<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent">

  <ImageView
    android:id="@+id/imageView6"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginStart="34dp"
    android:layout_marginLeft="34dp"
    android:layout_marginTop="32dp"
    android:layout_marginEnd="25dp"
    android:layout_marginRight="25dp"
    android:adjustViewBounds="true"
    android:scaleType="fitStart"
    app:layout_constraintEnd_toStartOf="@+id/imageView2"
    app:layout_constraintHeight_max="192dp"
    app:layout_constraintHorizontal_bias="0.5"
    app:layout_constraintHorizontal_chainStyle="packed"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:srcCompat="@drawable/ic_send_money" />

  <ImageView
    android:id="@+id/imageView2"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_marginStart="25dp"
    android:layout_marginLeft="25dp"
    android:layout_marginEnd="25dp"
    android:layout_marginRight="25dp"
    android:adjustViewBounds="true"
    android:scaleType="fitStart"
    app:layout_constraintBottom_toBottomOf="@+id/imageView6"
    app:layout_constraintEnd_toStartOf="@+id/imageView3"
    app:layout_constraintHorizontal_bias="0.5"
    app:layout_constraintStart_toEndOf="@+id/imageView6"
    app:layout_constraintTop_toTopOf="@+id/imageView6"
    app:srcCompat="@drawable/ic_recharges" />

  <ImageView
    android:id="@+id/imageView3"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_marginStart="25dp"
    android:layout_marginLeft="25dp"
    android:layout_marginEnd="34dp"
    android:layout_marginRight="34dp"
    android:scaleType="fitStart"
    app:layout_constraintBottom_toBottomOf="@+id/imageView2"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.5"
    app:layout_constraintStart_toEndOf="@+id/imageView2"
    app:layout_constraintTop_toTopOf="@+id/imageView2"
    app:srcCompat="@drawable/ic_my_card" />

  <ImageView
    android:id="@+id/imageView8"
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintBottom_toBottomOf="@+id/imageView9"
    app:layout_constraintEnd_toEndOf="@+id/imageView2"
    app:layout_constraintStart_toStartOf="@+id/imageView2"
    app:layout_constraintTop_toTopOf="@+id/imageView9"
    app:srcCompat="@drawable/ic_beneficiaries" />

  <ImageView
    android:id="@+id/imageView9"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="32dp"
    android:layout_marginBottom="32dp"
    android:adjustViewBounds="true"
    android:scaleType="fitStart"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="@+id/imageView6"
    app:layout_constraintHeight_max="192dp"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="@+id/imageView6"
    app:layout_constraintTop_toBottomOf="@+id/textView"
    app:layout_constraintVertical_bias="0.0"
    app:srcCompat="@drawable/ic_passbook" />

  <TextView
    android:id="@+id/textView"
    style="@style/TextAppearance.MaterialComponents.Caption"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:text="Send Money"
    app:layout_constraintEnd_toEndOf="@+id/imageView6"
    app:layout_constraintStart_toStartOf="@+id/imageView6"
    app:layout_constraintTop_toBottomOf="@+id/imageView6" />

  <TextView
    android:id="@+id/textView2"
    style="@style/TextAppearance.MaterialComponents.Caption"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:text="Recharge"
    app:layout_constraintEnd_toEndOf="@+id/imageView2"
    app:layout_constraintStart_toStartOf="@+id/imageView2"
    app:layout_constraintTop_toBottomOf="@+id/imageView2" />

  <TextView
    android:id="@+id/textView3"
    style="@style/TextAppearance.MaterialComponents.Caption"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:text="My Card"
    app:layout_constraintEnd_toEndOf="@+id/imageView3"
    app:layout_constraintStart_toStartOf="@+id/imageView3"
    app:layout_constraintTop_toBottomOf="@+id/imageView3" />

  <TextView
    android:id="@+id/textView4"
    style="@style/TextAppearance.MaterialComponents.Caption"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:text="Passbook"
    app:layout_constraintEnd_toEndOf="@+id/imageView9"
    app:layout_constraintStart_toStartOf="@+id/imageView9"
    app:layout_constraintTop_toBottomOf="@+id/imageView9" />

  <TextView
    android:id="@+id/textView5"
    style="@style/TextAppearance.MaterialComponents.Caption"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:text="Beneficiaries"
    app:layout_constraintEnd_toEndOf="@+id/imageView8"
    app:layout_constraintStart_toStartOf="@+id/imageView8"
    app:layout_constraintTop_toBottomOf="@+id/imageView8" />

</androidx.constraintlayout.widget.ConstraintLayout>
